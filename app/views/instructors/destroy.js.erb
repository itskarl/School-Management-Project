   $(this).closest('div').parent().prev().prev().prev('div').fadeOut();
   $(this).closest('div').parent().prev().prev().prev().prev('div').fadeOut();
   $(this).closest('div').parent().prev().prev().prev().prev().prev('div').fadeOut();
});
